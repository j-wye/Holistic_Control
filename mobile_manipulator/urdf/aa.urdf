              <action_monitor_rate>20.0</action_monitor_rate>
              <allow_partial_joints_goal>false</allow_partial_joints_goal>
              <constraints>
                <goal_time>0.0</goal_time>
                <stopped_velocity_tolerance>0.01</stopped_velocity_tolerance>
                <trajectory>
                  <arm_joint_1>
                    <max_velocity>0.5</max_velocity>
                    <max_acceleration>1.0</max_acceleration>
                  </arm_joint_1>
                  <arm_joint_2>
                    <max_velocity>0.5</max_velocity>
