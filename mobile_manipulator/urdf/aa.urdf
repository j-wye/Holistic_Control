                    <max_acceleration>1.0</max_acceleration>
                  </arm_joint_4>
                  <arm_joint_5>
                    <max_velocity>0.5</max_velocity>
                    <max_acceleration>10.0</max_acceleration>
                  </arm_joint_5>
                  <arm_joint_6>
                    <max_velocity>0.5</max_velocity>
                    <max_acceleration>10.0</max_acceleration>
                  </arm_joint_6>
                </trajectory>
              </constraints>
              <interpolator>
                <type>cubic</type>
              </interpolator>
            </joint_trajectory_controller>

            <twist_controller>
              <type>picknik_twist_controller/PicknikTwistController</type>
              <joint>tool_frame</joint>
              <interface_names>
                <name>twist.linear.x</name>
                <name>twist.linear.y</name>
                <name>twist.linear.z</name>
                <name>twist.angular.x</name>
                <name>twist.angular.y</name>
                <name>twist.angular.z</name>
              </interface_names>
              <constraints>
                <max_linear_velocity>0.3</max_linear_velocity>
                <max_angular_velocity>0.85</max_angular_velocity>
                <max_linear_acceleration>0.4</max_linear_acceleration>
                <max_angular_acceleration>0.4</max_angular_acceleration>
              </constraints>
            </twist_controller>

            <diff_drive_controller>
              <type>diff_drive_controller/DiffDriveController</type>
              <left_wheel_names>
                <wheel>fl_joint</wheel>
              </left_wheel_names>
              <right_wheel_names>
                <wheel>fr_joint</wheel>
              </right_wheel_names>
              <publish_limited_velocity>true</publish_limited_velocity>
              <wheel_separation>0.255094</wheel_separation>
              <wheel_radius>0.0575</wheel_radius>
              <cmd_vel_timeout>0.25</cmd_vel_timeout>
              <use_stamped_vel>false</use_stamped_vel>
              <base_frame_id>base_link</base_frame_id>
              <odom_frame_id>odom</odom_frame_id>
              <linear>
                <x>
                  <has_velocity_limits>true</has_velocity_limits>
                  <max_velocity>1.5</max_velocity>
                  <min_velocity>-1.5</min_velocity>
                </x>
              </linear>
              <angular>
                <z>
                  <has_velocity_limits>true</has_velocity_limits>
                  <max_velocity>0.78539816339</max_velocity>
                  <min_velocity>-0.78539816339</min_velocity>
                </z>
              </angular>
            </diff_drive_controller>
          </ros__parameters>
        </controller_manager>
      </ros2_control>
    </plugin>
    <plugin filename="libgazebo_ros_diff_drive.so" name="diff_drive_controller">
      <ros>
        <namespace>/</namespace>
        <remapping>cmd_vel:=/cmd_vel</remapping>
        <remapping>odom:=/odom</remapping>
      </ros>
